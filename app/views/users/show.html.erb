<%= render "devise/shared/header" %>
<h1>プロフィール</h1>

<h3>ユーザー名</h3>
<%= @user.nickname %>
<h3>メールアドレス</h3>
<%= @user.email %>
<h3>自己紹介</h3>
<%= @user.text%>
<p>
<% if user_signed_in? && current_user.id == @user.id %> 
             
     <div class="btn-group">
      <%= link_to 'プロフィール編集', edit_user_registration_path, method: :get, class: "btn btn-sm btn-outline-secondary" %>
     
     </div>
     <% end %>